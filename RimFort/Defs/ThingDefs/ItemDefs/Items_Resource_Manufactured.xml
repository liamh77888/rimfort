<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <!--=============== Medicine ====================-->

  <ThingDef ParentName="ResourceBase">
    <defName>GoldSalve</defName>
    <label>golden salve</label>
    <description>A pack of herbal concoctions useable as medicine.</description>
    <thingClass>Medicine</thingClass>
    <graphicData>
      <texPath>Things/Item/Resource/GoldSalve</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <soundInteract>Standard_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <MarketValue>12</MarketValue>
      <Mass>0.35</Mass>
      <Flammability>1.0</Flammability>
      <MedicalPotency>0.40</MedicalPotency>
      <DeteriorationRate>10</DeteriorationRate>
    </statBases>
    <thingCategories>
      <li>Medicine</li>
    </thingCategories>
    <tickerType>Rare</tickerType>
    <comps>
      <li Class="CompProperties_Rottable">
        <daysToRotStart>150</daysToRotStart>
				<rotDestroys>true</rotDestroys>
			</li>
    </comps>
  </ThingDef>
  
</Defs>